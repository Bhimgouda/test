coco LocalVariables

state persistent:
    numArr []U64

func localVars():
    var u U64
    u = 1
    var Bool  = true


// Local variables don't require datatype identifier
// if A value is assigned while declaring 
// eg: var u = 123

// But require the datatype identifier when declared
// without assigning a value
// eg: var u U4
// u = 121


endpoint invokable Add()->(sum U64):
    var num1 = [1,2,1]
    var num2 = 212
    sum = num1 + num2


